<?xml version='1.0' encoding='utf-8'?>
<widget id="tw.dac.kickstar" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>kickstar</name>
    <description>
		A sample Apache Cordova application that responds to the deviceready
		event.
	</description>
    <author email="thym-dev@eclipse.org" href="http://www.eclipse.org/thym">
		Eclipse.org - Thym
	</author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <feature name="Hello">
        <param name="android-package" value="com.example.plugin.Hello" />
        <param name="id" value="co.banpad.test" />
    </feature>
    <engine name="android" spec="4.1.1" />
    <plugin name="com.pylonproducts.wifiwizard" spec="~0.2.11" />
    <plugin name="cordova-plugin-background-mode" spec="https://github.com/katzer/cordova-plugin-background-mode" />
    <plugin name="cordova-plugin-globalization" spec="~1.0.3" />
    <plugin name="cordova-plugin-screen-orientation" spec="~1.4.2" />
    <plugin name="cordova-plugin-dialogs" spec="~1.2.1" />
</widget>
